@external h1, h2, h3, h4, h5, h6;

@defmixin fontSmooting() {
-webkit-font-smoothing: antialiased; /* Chrome 10+,Safari 5.1+ */
/* @alternate */ -moz-osx-font-smoothing: grayscale;
 }
 
body {
    padding-top: 20px;
    padding-bottom: 20px;
	font-family: 'Noto Sans', sans-serif;
}

.primaryLoader {
    gwt-sprite: "primaryLoader";
}

.brand {
    gwt-sprite: "brand";
}

.image {
    display: block;
    margin-left: auto;
    margin-right: auto;
}

p .emoji {
    height: 14px;
    margin-bottom: 2px;
}

h1 .emoji, .h1 .emoji {
    height: 36px;
    margin-bottom: 2px;
}

h2 .emoji, .h2 .emoji {
    height: 30px;
    margin-bottom: 2px;
}

h3 .emoji, .h3 .emoji {
    height: 24px;
    margin-bottom: 2px;
}

h4 .emoji, .h4 .emoji {
    height: 18px;
    margin-bottom: 2px;
}

h5 .emoji, .h5 .emoji {
    height: 14px;
    margin-bottom: 2px;
}

h6 .emoji, .h6 .emoji {
    height: 12px;
    margin-bottom: 2px;
}

.internalLink {
}

.externalLink:after {
position: relative;
  top: 2px;
  display: inline-block;
  font-family: 'Glyphicons Halflings';
  font-style: normal;
  font-size: smaller;
  font-weight: 400;
  line-height: 1;
  content: "\e144";
  margin-left: 3px;
  @mixin fontSmooting();
}