<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style gss="true"
		type="com.willshex.blogwt.client.part.property.CombinationPropertyPart.Style">
		.checkbox label {
			display: inline;
			padding-left: 5px;
			padding-right: 10px;
		}
		</ui:style>
	<g:HTMLPanel styleName="form-group">
		<label class="control-label" ui:field="elDescription" />
		<label class="small text-muted" ui:field="elName" />
		<g:HTMLPanel ui:field="pnlCheckBoxes">

		</g:HTMLPanel>
		<g:HTMLPanel styleName="control-label small" ui:field="pnlValueNote"
			visible="false" />
	</g:HTMLPanel>
</ui:UiBinder> 