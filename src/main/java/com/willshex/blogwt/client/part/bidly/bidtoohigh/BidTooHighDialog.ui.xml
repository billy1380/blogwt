<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:com.willshex.blogwt.client.part.datefield">
	<ui:style gss="true">
	.text-upper {
		text-transform: uppercase;
	}
	</ui:style>
	<g:HTMLPanel styleName="modal fade">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<g:Button ui:field="btnClose" styleName="close">
						<span class="glyphicon glyphicon-remove" />
					</g:Button>
					<h3 ui:field="pageTitle" class="{style.text-upper}">Bid too high</h3>
				</div>
				<g:FormPanel>
					<g:HTMLPanel>
						<div class="modal-body">
							<g:HTMLPanel ui:field="pnlBid" styleName="form-group">
								<label class="control-label">Bid</label>
								<span class="input-group">
									<span class="input-group-addon">Â£</span>
									<g:TextBox ui:field="txtBid" styleName="form-control" />
								</span>
								<g:HTMLPanel styleName="control-label small" ui:field="pnlBidNote"
									visible="false" />
							</g:HTMLPanel>

							<g:HTMLPanel styleName="form-group">
								<label class="control-label" ui:field="elDeliveryDate">Delivery
									date</label>
								<g:ListBox ui:field="cboDeliveryDate" styleName="form-control">
									<g:item value="1w">1 week</g:item>
									<g:item value="1m">1 month</g:item>
									<g:item value="3m">3 months</g:item>
								</g:ListBox>
								<g:HTMLPanel styleName="control-label small" ui:field="pnlDeliveryDateNote"
									visible="false" />
							</g:HTMLPanel>
							<g:HTMLPanel styleName="form-group">
								<label class="control-label" ui:field="elExpiryDate">Expiry Date</label>
								<d:DateField ui:field="ctlExpiryDate" />
								<g:HTMLPanel styleName="control-label small" ui:field="pnlExpiryDateNote"
									visible="false" />
							</g:HTMLPanel>
						</div>
						<div class="modal-footer">
							<g:SubmitButton ui:field="btnUpdate" styleName="btn btn-primary pull-right">
								Submit
							</g:SubmitButton>
						</div>
					</g:HTMLPanel>
				</g:FormPanel>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 